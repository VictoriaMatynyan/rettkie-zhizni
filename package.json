{
  "name": "rettkie-zhizni",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "eslint . --ext .js,.vue",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "format": "prettier --write \"src/**/*.{js,vue,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue,css,html}\"",
    "json-server": "json-server --watch db.json --port 3001",
    "dev:full": "concurrently \"npm run dev\" \"npm run json-server\""
  },
  "engines": {
    "node": "22.17.1",
    "npm": ">=10.9.0"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "pinia": "^3.0.3",
    "v-mask": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-vue": "^10.3.0",
    "gh-pages": "^6.3.0",
    "json-server": "^1.0.0-beta.3",
    "postcss": "^8.5.3",
    "prettier": "^3.6.2",
    "vite": "^6.3.5"
  }
}
